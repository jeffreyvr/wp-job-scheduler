{
    "name": "jeffreyvanrossum/wp-job-scheduler",
    "description": "Handy wrapper to make creating WordPress job scheduling and dispatch a breeze.",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeffrey van Rossum",
            "email": "jeffrey@vanrossum.dev"
        }
    ],
    "require": {
        "php": "^8.0",
        "woocommerce/action-scheduler": "^3.7"
    },
    "require-dev": {
        "phpcompatibility/php-compatibility": "*",
        "laravel/pint": "^1.4",
        "spatie/ray": "^1.36"
    },
    "autoload": {
        "psr-4": {
            "Jeffreyvr\\WPJobScheduler\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "format": "./vendor/bin/pint ./src/"
    }
}
